{
  "env": {
      "node": true,
      "es6": true,
      "jest": true
  },
  "plugins": ["jest", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "project": "./tsconfig.json"
  },
  "rules": {
      "require-jsdoc": 0
  }
}